docker.runOptions = '-u $(id -u):$(id -g)'
docker.enabled = true
process.scratch = true
conda.enabled = true
nextflow.enable.moduleBinaries = true

params {
    sample_name = 'NB-8204-M'
    library = "S-24-0479"
    proband = "SAMPLE"
    tissue = "muscle"
    meta = params.sample_name + "-" + params.tissue
    fastq_bucket = "s3://ucla-rare-diseases/UCLA-UDN/rnaseq/fastq"
    rib_reference_path = "s3://ucla-rare-diseases/UCLA-UDN/assets/reference"
    gencode_gtf_path = "s3://ucla-rare-diseases/UCLA-UDN/assets/reference/gencode43/GRCh38.p13/gencode.v43.primary_assembly.annotation.gtf"
    gencode_gtf_collapse = "s3://ucla-rare-diseases/UCLA-UDN/assets/reference/gencode43/GRCh38.p13/gencode.v43.primary_assembly.annotation.collapse.gtf"
    outdir = "results"
    human_fai = "s3://ucla-rare-diseases/UCLA-UDN/assets/reference/gencode43/GRCh38.p13/GRCh38.primary_assembly.genome.fa.fai"
    human_dict = "s3://ucla-rare-diseases/UCLA-UDN/assets/reference/gencode43/GRCh38.p13/GRCh38.primary_assembly.genome.dict"
    human_fasta = "s3://ucla-rare-diseases/UCLA-UDN/assets/reference/gencode43/GRCh38.p13/GRCh38.primary_assembly.genome.fa"
    output_bucket = "s3://ucla-rare-diseases/UCLA-UDN/Analysis/UDN_cases"
    features_master_file = "s3://ucla-rare-diseases/UCLA-UDN/gcarvalho_test/drop/test/fibroblast/featureCounts_fibroblast_24-07-22.tsv"
    ir_ref = "s3://ucla-rare-diseases/UCLA-UDN/assets/IRFinder-1.3.1/REF/GRCh38.p13/"
    bed = "s3://ucla-rare-diseases/UCLA-UDN/gcarvalho_test/rnaseq/hg38/XBP1-NM_001079539-hg38.bed"
    use_cram = false
    cram = null
}